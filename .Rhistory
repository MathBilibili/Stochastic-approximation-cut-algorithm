if(any(W_n>(10^(250+n_trun)))){
W_n<-rep(1,length(W))            #truncated here
n_trun<-n_trun+1
}
out<-list(t=t_n,I=I_n,W=W_n,n_trun=n_trun,coin=coin)
return(out)
}
